CHOREOGRAPHY {
	configuration ParrotArdrone2;
	import moves;

	CONTEXT {
		maxLength = 30;
		maxHeight = 30;
		maxWidth = 30;
		positionX = 10;
		initialDirection = BEHIND;
		positionY = 10;
	}
	part{
		(backward ( distance = 8 temps = 5) then forward ( distance = 8 temps = 5));
		merge( rotate ( angle = 180 temps = 6), up ( distance = 5 temps = 3) );
		wait(temps = 4);
		part2 X 2;
		part3 with Cam ( mode ON, id = Bottom);
	}
	part2{
		rotate(angle = 90 temps = 2);
		left(distance = 6 temps = 4);
	}
	part3{
		right ( distance = 8 temps = 5) with Cam ( mode OFF, id = Front );
		down ( distance = 2 temps = 2);
	}
	start = part;
}